require_relative '../lib/board.rb'

objGame = Game.new()
objBoard = Board.new()

board = objBoard.get_board()

def display_board(board)
  puts " #{board[0]} | #{board[1]} | #{board[2]} "
  puts '-----------'
  puts " #{board[3]} | #{board[4]} | #{board[5]} "
  puts '-----------'
  puts " #{board[6]} | #{board[7]} | #{board[8]} "
end

display_board(board)



